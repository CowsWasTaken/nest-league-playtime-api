// This is your Prisma schema file,
// learn more about it in the docs https//pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Match {
    matchId String @id
    gameDuration BigInt
    gameCreation BigInt
    gameEndTimestamp BigInt
    gameId BigInt
    gameName String
    gameType String
    gameMode String
    queueId Int
    matchSummonerLink MatchSummonerLink[]
    mapId Int?
}

model Account {
    puuid String @id
    gameName String 
    tagLine String
    matchSummonerLink MatchSummonerLink[]
}

model MatchSummonerLink {
    account Account @relation(fields: [puuid], references: [puuid])
    match Match @relation(fields: [matchId], references: [matchId])
    puuid String 
    matchId String
    @@id([puuid, matchId])
}