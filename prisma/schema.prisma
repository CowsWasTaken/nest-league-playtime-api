// This is your Prisma schema file,
// learn more about it in the docs https//pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Match {
  matchId            String     @id
  gameDuration       BigInt
  gameCreation       BigInt
  gameEndTimestamp   BigInt?
  gameStartTimestamp BigInt
  gameId             BigInt
  gameName           String
  gameType           String
  gameMode           String
  queueId            Int
  summoner           Summoner[]
  mapId              Int?
}

model Summoner {
  puuid    String  @id
  gameName String
  tagLine  String
  matches  Match[]
}
